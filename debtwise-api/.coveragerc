[run]
source = app
omit = 
    */tests/*
    */test_*
    */__init__.py
    */alembic/*
    */scripts/*
    app/main.py

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    except ImportError:
    pass

[html]
directory = htmlcov

[xml]
output = coverage.xml